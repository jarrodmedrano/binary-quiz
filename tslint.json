{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "jsx-wrap-multiline": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "trailing-comma": [
      false
    ]
  },
  "linterOptions": {
    "exclude": [
      "node_modules",
      "build",
      "scripts",
      "acceptance-tests",
      "webpack",
      "jest",
      "src/setupTests.js",
      "src/serviceWorker.js"
    ]
  }
}